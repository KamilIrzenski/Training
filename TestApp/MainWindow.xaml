        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="9*"/>
            <ColumnDefinition Width="159*"/>
            <ColumnDefinition Width="626*"/>
        </Grid.ColumnDefinitions>
        <TextBox HorizontalAlignment="Left" Height="22" Margin="41.2,147,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="236" Grid.Column="1" Grid.ColumnSpan="2" AutomationProperties.Name="testBoxData"/>
